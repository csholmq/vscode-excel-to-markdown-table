{
    "name": "excel-to-markdown-table",
    "displayName": "Excel to Markdown table",
    "description": "Copy Excel data and convert to Markdown table format",
    "version": "1.1.0",
    "publisher": "csholmq",
    "repository": {
        "url": "https://github.com/csholmq/vscode-excel-to-markdown-table"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onLanguage:feature"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "key": "shift+alt+v",
                "command": "extension.excelToMarkdown",
                "title": "Excel to Markdown table",
                "when": "editorLangId == 'markdown'"
            },
            {
                "key": "shift+alt+v",
                "command": "extension.excelToMarkdown",
                "title": "Excel to feature table",
                "when": "editorLangId == 'feature'"
            }
        ],
        "commands": {
            "command": "extension.excelToMarkdown",
            "title": "Excel to Markdown table"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "mocha": "./node_modules/.bin/mocha --opts ./src/test/mocha.opts",
        "coverage": "./node_modules/.bin/nyc npm run mocha"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "nyc": "^13.0.1",
        "source-map-support": "^0.5.9",
        "ts-node": "^7.0.1",
        "typescript": "^2.5.3",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "**/*.test.ts",
            "src/test/index.ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    }
}
