{
    "name": "excel-to-markdown-table",
    "displayName": "Excel to Markdown table",
    "description": "Copy Excel data and convert to Markdown table format",
    "version": "1.1.0",
    "publisher": "csholmq",
    "repository": {
        "url": "https://github.com/csholmq/vscode-excel-to-markdown-table"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": {
            "key": "shift+alt+v",
            "command": "extension.excelToMarkdown",
            "title": "Excel to Markdown table",
            "when": "editorLangId == 'markdown'"
        },
        "commands": {
            "command": "extension.excelToMarkdown",
            "title": "Excel to Markdown table"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "mocha": "./node_modules/.bin/mocha --opts ./src/test/mocha.opts",
        "coverage": "./node_modules/.bin/nyc npm run mocha"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.4",
        "@types/vscode": "^1.41.0",
        "nyc": "^15.0.0",
        "source-map-support": "^0.5.16",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.d.ts",
            "**/*.test.ts",
            "src/test/index.ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    }
}
